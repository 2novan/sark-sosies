<%= simple_form_for [user, sosie, booking] do |f|  %>
  <%= f.text_field :start_date, data:{ provide:'datepicker' } %>
  <%= f.text_field :end_date, data:{ provide:'datepicker' } %>
  <%= f.input :total_price, disabled: true %>
  <%= f.input :address %>
  <%= f.input :description %>
  <%= f.submit %>
<% end %>
