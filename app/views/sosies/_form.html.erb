<%= simple_form_for [@user, @sosie], html: { class: "booking-form"} do |f|  %>
  <%= f.input :name%>
  <%= f.input :address %>
  <%= f.input :description %>
  <%= f.input :price_per_day %>
  <%= f.input :photo, as: :file %>
  <%= f.submit "Book Now", input_html: { class: 'btn-booking' } %>
<% end %>
